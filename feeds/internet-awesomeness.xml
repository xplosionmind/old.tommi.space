---
url: /internet-awesomeness-feed
title: Internet Awesomeness feed
description: Internet Awesomeness entries
---
<?xml version='1.0' encoding='utf-8'?>
<rss version='2.0'>
	<channel>
		<title>Internet Awesomeness — {{ site.title }}</title>
		<description>{{ "Entries of Tommi’s Internet Awesomeness, a collection of links to awesome stuff on the internet." | xml_escape }}</description>
		<link>https://tommi.space/internet-awesomeness</link>
		{% assign awesomeness = site.data.internet-awesomeness | sort: 'added' | reverse %}
		<lastBuildDate>{% for entry in awesomeness limit:1 %}{{ entry.added | date_to_rfc822 }}{% endfor %}</lastBuildDate>
		<image>
			<url>{{ site.image | absolute_url }}</url>
			<title>Internet Awesomeness — {{ site.title }}</title>
			<link>https://tommi.space/internet-awesomeness</link>
		</image>
		<language>en</language>
		<managingEditor>{{ site.email }} (Tommi)</managingEditor>
		<webMaster>{{ site.email }} (Tommi)</webMaster>
		{% for website in awesomeness %}
			<item>
				<title>{{ website.title | xml_escape }}</title>
				{% if website.added %}<pubDate>{{ website.added | date_to_rfc822 }}</pubDate>{% endif %}
				<description>{{ website.description | xml_escape | strip_newlines }}</description>
				<link>{{ website.url }}</link>
				<guid>{{ website.url }}</guid>
			</item>
		{% endfor %}
	</channel>
</rss>
